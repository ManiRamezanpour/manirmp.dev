    "rehype": "^12.0.1",
    "rehype-accessible-emojis": "^0.3.2",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-pretty-code": "^0.9.4",
    "rehype-slug": "^5.1.0",
    "remark-gfm": "^3.0.1",
    "remark-unwrap-images": "^3.0.1",
    "remove-markdown": "^0.5.0",
    "rimraf": "^4.4.1",
