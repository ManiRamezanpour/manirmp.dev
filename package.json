    "@vercel/og": "^0.5.0",
    "canvas-confetti": "^1.6.0",
    "classix": "^2.1.24",
    "colord": "^2.9.3",
    "contentlayer": "^0.3.1",
    "country-locale-map": "^1.8.11",
    "kysely": "^0.24.2",
    "kysely-planetscale": "^1.3.0",
    "next": "^13.3.0",
    "next-contentlayer": "^0.3.1",
    "next-themes": "0.2.1",
